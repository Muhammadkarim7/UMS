<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".HomeFragment">

    <TextView
        android:id="@+id/txt_ums"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:text="UMS"
        android:textColor="#f21"
        android:textStyle="bold"
        android:textSize="100dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />

    <ImageView
        android:id="@+id/shop"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@drawable/baseline_shopping_bag_24"
        android:layout_margin="50dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/txt_ums"
        app:layout_constraintRight_toLeftOf="@id/time"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TARIFlAR"
        app:layout_constraintTop_toBottomOf="@id/shop"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/time"
        android:layout_marginTop="10dp"
        android:layout_marginRight="45dp"
        />

    <ImageView
        android:id="@+id/time"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_margin="50dp"
        android:src="@drawable/baseline_access_time_24"
        app:layout_constraintLeft_toRightOf="@id/shop"
        app:layout_constraintTop_toBottomOf="@id/txt_ums"
        app:layout_constraintRight_toRightOf="parent"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="DAQIQALAR"
        app:layout_constraintTop_toBottomOf="@id/time"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="10dp"
        android:layout_marginRight="60dp"
        />

    <ImageView
        android:id="@+id/internet"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@drawable/baseline_speaker_phone_24"
        android:layout_margin="50dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/shop"
        app:layout_constraintRight_toLeftOf="@id/xizmat"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="INTERNET"
        app:layout_constraintTop_toBottomOf="@id/internet"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/time"
        android:layout_marginTop="10dp"
        android:layout_marginRight="50dp"
        />

    <ImageView
        android:id="@+id/xizmat"
        android:layout_width="100dp"
        android:layout_margin="50dp"
        android:layout_height="100dp"
        android:src="@drawable/baseline_settings_24"
        app:layout_constraintLeft_toRightOf="@id/internet"
        app:layout_constraintTop_toBottomOf="@id/time"
        app:layout_constraintRight_toRightOf="parent"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="XIZMATLAR"
        app:layout_constraintTop_toBottomOf="@id/xizmat"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="10dp"
        android:layout_marginRight="65dp"
        />

    <ImageView
        android:id="@+id/sms"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_margin="50dp"
        android:src="@drawable/baseline_local_post_office_24"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/internet"
        app:layout_constraintRight_toLeftOf="@id/news"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="SMS"
        app:layout_constraintTop_toBottomOf="@id/sms"
        app:layout_constraintLeft_toLeftOf="parent"
        android:textSize="15dp"
        app:layout_constraintRight_toLeftOf="@id/time"
        android:layout_marginRight="50dp"
        />

    <ImageView
        android:id="@+id/news"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@drawable/baseline_newspaper_24"
        app:layout_constraintLeft_toRightOf="@id/sms"
        android:layout_margin="50dp"
        app:layout_constraintTop_toBottomOf="@id/xizmat"
        app:layout_constraintRight_toRightOf="parent"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="YANGILIKLAR"
        app:layout_constraintTop_toBottomOf="@id/news"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="10dp"
        android:layout_marginRight="60dp"
        />


</androidx.constraintlayout.widget.ConstraintLayout>